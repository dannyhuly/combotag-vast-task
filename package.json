{
  "name": "nest-typescript-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "start": "node index.js",
    "start:watch": "nodemon",
    "prestart:prod": "tsc",
    "start:prod": "node dist/server.js",
    "test:unit": "node_modules/.bin/ts-node node_modules/.bin/jasmine --config=jasmine.json",
    "test:unit:coverage": "node_modules/.bin/nyc --nycrc-path=nyc-conf.json npm run test:unit",
    "test:integ": "node_modules/.bin/ts-node node_modules/.bin/jasmine --config=jasmine.json"
  },
  "dependencies": {
    "@nestjs/common": "^4.4.0",
    "@nestjs/core": "^4.4.0",
    "@nestjs/microservices": "^4.4.0",
    "@nestjs/testing": "^4.4.0",
    "@nestjs/websockets": "^4.4.0",
    "joi": "^13.1.2",
    "redis": "^2.7.1",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.4.0",
    "typescript": "^2.6.2"
  },
  "devDependencies": {
    "@types/express": "^4.11.0",
    "@types/jasmine": "^2.8.2",
    "@types/joi": "^13.0.5",
    "@types/node": "^8.0.28",
    "@types/request": "^2.0.9",
    "@types/supertest": "^2.0.4",
    "jasmine": "^2.8.0",
    "nodemon": "^1.12.1",
    "nyc": "^11.4.1",
    "request": "^2.83.0",
    "supertest": "^3.0.0",
    "ts-node": "^3.3.0"
  },
  "engines": {
    "node": ">=8.9.4",
    "npm": ">=5.6.0"
  }
}